<!-- configurable section -->
<script type="text/javascript">
if (typeof rtapi == 'undefined') var rtapi = {}; rtapi.kwhL = {};
rtapi.kwhL.title = 'Productivity  by day';
rtapi.kwhL.kind = 'sidebar';
rtapi.kwhL.styling = { width:300, height:300 };
</script>
<!-- below is required -->
<div id="rtdivkwhL" style="float:left"></div>
<script src="https://www.rescuetime.com/anapi/js/embed/B63whLoHfLQPzwoehwQEjGQX_dSTeGA6nIxwgR5z" type="text/javascript"></script><script type='text/javascript'>RTAPI.render_all(rtapi);</script>

<div style="float:left">
  <% @events['twinge'].each do |date, events| %>
    <h3><%= date.strftime('%a, %b %e %Y') %></h3>
    <ul>
    <% events.each do |event| %>
      <% event.payload.commits.select(&:distinct).each do |commit| %>
        <li>
          <%= event.repo.name.split('/').last %> -
          <%= commit.message %>
        </li>
      <% end %>
    <% end %>
    </ul>
  <% end %>
</div>
